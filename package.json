{
  "name": "vite_vue_template",
  "author": "Dave Holden",
  "publisher": "dave-holden",
  "displayName": "Vite Vue3 Template",
  "description": "Vite Vue3 Template with ESLint and Prettier",
  "license": "MIT",
  "version": "0.0.0",
  "keywords": [
    "vue",
    "template"
  ],
  "packageManager": "pnpm@7.5.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/dave-holden/vite_vue_template"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}"
  },
  "engines": {
    "node": "18.4.0"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "prettier": "^2.7.1",
    "vite": "^3.0.2"
  }
}
